<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %></title>
</head>
<body>

    <h1>Hello, <%= name %>!</h1>

    <% if (isAdmin) { %>
        <p>Welcome, administrator!</p>
    <% } else { %>
        <p>You are not an administrator.</p>
    <% } %>

    <ul>
        <% fruits.forEach(function(fruit) { %>
            <li><%= fruit %></li>
        <% }); %>
    </ul>

</body>
</html>